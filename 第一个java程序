public class java {
    public static void main(String...args){
        char grade = 'A';
        switch (grade){
            case 'A' ->System.out.println("100~96");
            case 'B' ->System.out.println("95~89");
            case 'C' ->System.out.println("88~79");
            case 'D' ->System.out.println("78~60");
            case 'F' ->System.out.println("60以下");

        }
        System.out.println("你的等级"+grade);




    }
}
